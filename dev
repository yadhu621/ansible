[loadbalancer]
lb01 ansible_ssh_private_key_file=/root/.ssh/id_rsa

[webserver]
app01 ansible_ssh_private_key_file=/root/.ssh/id_rsa
app02 ansible_ssh_private_key_file=/root/.ssh/id_rsa

[database]
db01 ansible_ssh_private_key_file=/root/.ssh/id_rsa

[controlmachine]
control ansible_connection=local